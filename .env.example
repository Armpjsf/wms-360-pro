# Google Service Account Credentials
GOOGLE_SERVICE_ACCOUNT_EMAIL=your-service-account@your-project.iam.gserviceaccount.com
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYour private key here\n-----END PRIVATE KEY-----\n"

# Google Sheets IDs
NEXT_PUBLIC_PO_SPREADSHEET_ID=your-spreadsheet-id-here
NEXT_PUBLIC_DELIVERY_FOLDER_ID=your-folder-id-here

# Mobile Build Flag (set to "true" for mobile APK build)
NEXT_PUBLIC_MOBILE_BUILD=false

# Production URL (for Vercel deployment)
NEXT_PUBLIC_PRODUCTION_URL=https://your-app.vercel.app
